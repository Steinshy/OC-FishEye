{
  "extends": ["html-validate:recommended"],
  "rules": {
    "doctype-style": "off",
    "void-style": "off",
    "no-redundant-role": "off",
    "prefer-native-element": "off",
    "aria-label-misuse": "off",
    "element-required-attributes": "warn",
    "wcag/h30": "warn",
    "wcag/h32": "warn",
    "wcag/h36": "warn",
    "wcag/h37": "warn",
    "wcag/h67": "warn",
    "wcag/h71": "warn"
  }
}
