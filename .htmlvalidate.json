{
  "extends": ["html-validate:recommended"],
  "rules": {
    "doctype-style": "off",
    "element-case": "error",
    "attr-case": "error",
    "attr-quotes": "error",
    "void-content": "off",
    "void-style": "off",
    "no-dup-attr": "error",
    "no-dup-class": "error",
    "no-dup-id": "error",
    "no-unknown-elements": "error",
    "no-unknown-attributes": "error",
    "no-raw-text": "off",
    "require-sri": "off",
    "no-inline-style": "warn",
    "no-inline-script": "warn",
    "no-missing-close": "error",
    "no-self-closing": "off",
    "no-trailing-whitespace": "error",
    "no-unused-disable": "error",
    "prefer-tbody": "off",
    "prefer-native-element": "off",
    "require-meta-charset": "error",
    "require-meta-description": "warn",
    "require-meta-viewport": "warn",
    "require-title": "error",
    "text-content": "warn",
    "aria-label-misuse": "off",
    "no-redundant-role": "off",
    "element-required-attributes": "warn",
    "wcag/h30": "warn",
    "wcag/h32": "warn",
    "wcag/h36": "warn",
    "wcag/h37": "warn",
    "wcag/h67": "warn",
    "wcag/h71": "warn"
  }
}
