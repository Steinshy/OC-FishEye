{
  "name": "fisheye",
  "version": "1.0.0",
  "description": "FishEye photography website",
  "type": "module",
  "main": "index.html",
  "scripts": {
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "changelog": "conventional-changelog --preset angular --output-format json --release-count 0",
    "changelog:markdown": "conventional-changelog --preset angular --output-format markdown --release-count 0",
    "version": "conventional-changelog --preset angular --infile CHANGELOG.md --same-file"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.34.0",
    "conventional-changelog-angular": "^8.0.0",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-html": "^8.1.3",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.0.11",
    "lint-staged": "^16.1.5",
    "yocto-queue": "^0.1.0",
    "prettier": "^3.2.5"
  }
}
