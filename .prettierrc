{
  "semi": true,
  "trailingComma": "es5",
  "singleQuote": true,
  "printWidth": 160,
  "tabWidth": 2,
  "useTabs": false,
  "endOfLine": "lf",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "avoid",
  "quoteProps": "as-needed",
  "jsxSingleQuote": true,
  "htmlWhitespaceSensitivity": "css",
  "embeddedLanguageFormatting": "auto",
  "proseWrap": "preserve",
  "insertPragma": false,
  "requirePragma": false,
  "overrides": [
    {
      "files": ["*.html", "*.htm"],
      "options": {
        "parser": "html",
        "htmlWhitespaceSensitivity": "strict",
        "printWidth": 160,
        "bracketSameLine": true
      }
    },
    {
      "files": ["*.css", "*.scss", "*.sass", "*.less"],
      "options": {
        "parser": "css",
        "printWidth": 160,
        "singleQuote": false
      }
    },
    {
      "files": ["*.json", "*.jsonc"],
      "options": {
        "parser": "json",
        "printWidth": 160,
        "singleQuote": false
      }
    },
    {
      "files": ["*.js", "*.mjs", "*.cjs"],
      "options": {
        "parser": "babel",
        "printWidth": 160,
        "trailingComma": "es5"
      }
    },
    {
      "files": ["*.ts", "*.tsx"],
      "options": {
        "parser": "typescript",
        "printWidth": 160,
        "trailingComma": "es5"
      }
    },
    {
      "files": ["*.md", "*.mdx"],
      "options": {
        "parser": "markdown",
        "proseWrap": "always",
        "printWidth": 160,
        "singleQuote": false
      }
    },
    {
      "files": ["*.yml", "*.yaml"],
      "options": {
        "parser": "yaml",
        "printWidth": 160,
        "singleQuote": false
      }
    },
    {
      "files": ["*.xml", "*.svg"],
      "options": {
        "parser": "html",
        "printWidth": 160,
        "htmlWhitespaceSensitivity": "strict"
      }
    }
  ]
}
